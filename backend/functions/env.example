# Backend Environment Variables
# Copy this file to .env and fill in your actual values

# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# Google Cloud AutoML Configuration
AUTOML_ENDPOINT=your_automl_endpoint_here
AUTOML_MODEL_ID=your_automl_model_id_here
AUTOML_PROJECT_ID=your_automl_project_id_here
AUTOML_REGION=your_automl_region_here

# Service Account Configuration
SERVICE_ACCOUNT_EMAIL=your_service_account_email_here
SERVICE_ACCOUNT_PRIVATE_KEY=your_service_account_private_key_here

# Firebase Configuration
FIREBASE_PROJECT_ID=your_firebase_project_id_here
